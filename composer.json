{
    "name": "elephox/plane",
    "description": "Docker files for running a basic Elephox application.",
    "license": "MIT",
    "keywords": [
        "elephox",
        "docker"
    ],
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Ricardo Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "support": {
        "issues": "https://github.com/elephox-dev/plane/issues",
        "source": "https://github.com/elephox-dev/plane"
    },
    "require": {
        "php": "^8.1 <8.3",
        "elephox/console": "^0.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Elephox\\Plane\\": "src/"
        }
    },
    "bin": [
        "bin/plane"
    ]
}
